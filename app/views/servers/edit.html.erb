<h1>編集画面</h1>

<%= form_with model: @server, url: server_path(@server.id), method: :patch do |f| %>

  <h4>ゲーム名</h4>
  <%= f.text_field :game_name %>

  <h4>サーバー名</h4>
  <%= f.text_field :server_name %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>コメント</h4>
  <%= f.text_area :body %>

  <%= f.submit '保存' %>

<% end %>
