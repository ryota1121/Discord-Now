<h1>新規投稿</h1>

<%= form_with model: @server, url: '/servers', method: :post do |f| %>
  <h4>ゲーム名</h4>
  <%= f.text_field :game_name %>

  <h4>サーバー名</h4>
  <%= f.text_field :server_name %>

  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>コメント</h4>
  <%= f.text_area :body %>

  <%= f.submit '投稿' %>
<% end %>
